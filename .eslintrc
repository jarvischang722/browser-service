{
    "extends": "airbnb",
    "globals": {
        "__DEV__": true,
        "db": true,
        "describe": true,
        "it": true,
        "should": true
    },
    "env": {
        "es6": true,
        "node": true
    },
    "ecmaFeatures": {
        "modules": true
    },
    "rules": {
        "no-confusing-arrow": 0,
        "semi": [2, "never"],
        "global-require": 0,
        "indent": [2, 4],
        "linebreak-style": "off"
    }
}